        <Rectangle x:Name="c_10" Grid.Row="1" Grid.Column="9" Fill="{ThemeResource SystemControlHighlightListAccentMediumBrush}" Height="Auto" Stroke="Black" Width="Auto" PointerPressed="Rectangle_PointerPressed"/>
